{
  "name": "dinnerwalks",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "clean:public": "rm -rf \"../public/**/*.*!(md)\" && rm -rf \"../public/_nuxt\"",
    "clean:functions": "rm -rf \"../functions/node_modules\" && rm -rf \"../functions/.nuxt\"",
    "clean:static": "rm -rf \"../src/static/sw.js\"",
    "clean": "npm run clean:public && npm run clean:functions && npm run clean:static",
    "copy:nuxt": "mkdir ../public/_nuxt && cp -r ./.nuxt/dist/* ../public/_nuxt && mkdir ../functions/.nuxt && cp -r ./.nuxt/dist ../functions/.nuxt",
    "copy:static": "cp -r ../src/static/* ../public",
    "copy": "npm run copy:nuxt && npm run copy:static",
    "build:firebase": "yarn run clean && yarn install && yarn run build && yarn run copy && cd ../functions && npm install && npm prune",
    "start:firebase": "cd ../ && firebase serve",
    "deploy": "firebase deploy --only functions,hosting"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-0",
    "@babel/preset-env": "~7.12.0",
    "@firebase/database": "^0.9.6",
    "@nuxtjs/firebase": "^7.5.0",
    "axios": "^0.21.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-vue": "^2.0.2",
    "core-js": "3",
    "eslint": "^7.23.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-vue": "^7.8.0",
    "firebase": "^8.2.7",
    "jquery": "^3.6.0",
    "nuxt": "^2.15.3",
    "nuxt-sweetalert2": "^1.0.0",
    "vue": "^2.6.12",
    "vue-recaptcha-v3": "^1.9.0",
    "vue-router": "^3.5.1",
    "vue-scrollto": "^2.20.0",
    "vue-sweetalert2": "^4.2.1",
    "webpack": "^5.28.0"
  },
  "devDependencies": {
    "@nuxtjs/pwa": "^3.3.5"
  }
}
